{
  "name": "solo-leveling-dsa",
  "version": "1.0.0",
  "description": "Gamified DSA learning platform inspired by Solo Leveling",
  "scripts": {
    "install:all": "cd server && npm install && cd ../client && npm install",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "seed": "cd server && node seeds/seedProblems.js",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "dsa",
    "learning",
    "solo-leveling",
    "gamification",
    "algorithms"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
